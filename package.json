{
  "name": "kindredcard",
  "version": "0.0.1",
  "description": "Personal CRM for managing contacts and relationships",
  "scripts": {
    "build:css": "tailwindcss -i web/static/css/input.css -o web/static/css/output.css",
    "watch:css": "tailwindcss -i web/static/css/input.css -o web/static/css/output.css --watch",
    "build:css:prod": "NODE_ENV=production tailwindcss -i web/static/css/input.css -o web/static/css/output.css --minify"
  },
  "keywords": [
    "crm",
    "contacts",
    "carddav"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "daisyui": "^4.6.0",
    "tailwindcss": "^3.4.1"
  }
}
